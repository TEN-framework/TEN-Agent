                "name": "flush"
              }
            ],
            "data": [
              {
                "dest": [
                  {
                    "extension": "message_collector"
                  },
                  {
                    "extension": "fashionai"
                  }
                ],
                "name": "text_data"
              }
            ],
            "extension": "openai_chatgpt"
          },
          {
            "data": [
              {
                "dest": [
                  {
                    "extension": "agora_rtc"
                  }
                ],
                "name": "data"
              }
            ],
            "extension": "message_collector"
          },
          {
            "cmd": [
              {
                "dest": [
                  {
                    "extension": "openai_chatgpt"
                  }
