            ]
          },
          {
            "extension": "agora_sess_ctrl",
            "audio_frame": [
              {
                "name": "pcm_frame",
                "dest": [
                  {
                    "extension": "minimax_v2v_python"
                  }
                ]
              }
            ],
            "cmd": [
              {
                "name": "start_of_sentence",
                "dest": [
                  {
                    "extension": "minimax_v2v_python",
                    "msg_conversion": {
                      "type": "per_property",
                      "keep_original": true,
                      "rules": [
                        {
                          "path": "_ten.name",
                          "conversion_mode": "fixed_value",
                          "value": "flush"
