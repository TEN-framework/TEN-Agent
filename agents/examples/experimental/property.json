                  {
                    "extension": "openai_chatgpt"
                  },
                  {
                    "extension": "message_collector"
                  }
                ],
                "name": "text_data"
              }
            ],
            "cmd": [
              {
                "name": "on_user_joined",
                "dest": [
                  {
                    "extension": "openai_chatgpt"
                  }
                ]
              },
              {
                "name": "on_user_left",
                "dest": [
                  {
                    "extension": "openai_chatgpt"
                  }
                ]
              }
            ],
            "extension": "agora_rtc"
