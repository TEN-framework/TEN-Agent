          {
            "addon": "fashionai",
            "extension_group": "default",
            "name": "fashionai",
            "property": {
              "app_id": "${env:AGORA_APP_ID}",
              "channel": "ten_agent_test",
              "stream_id": 12345,
              "token": "<agora_token>",
              "service_id": "agoramultimodel"
