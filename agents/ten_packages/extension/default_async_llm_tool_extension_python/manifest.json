{
  "type": "extension",
  "name": "default_async_llm_tool_extension_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.1.0"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "requirements.txt",
      "**.tent",
      "**.py",
      "README.md"
    ]
  },
  "api": {
    "property": {},
    "cmd_in": [
      {
        "name": "tool_call",
        "property": {
          "name": {
            "type": "string"
          },
          "arguments": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "arguments"
        ],
        "result": {
          "property": {
            "tool_result": {
              "type": "string"
            }
          },
          "required": [
            "tool_result"
          ]
        }
      }
    ],
    "cmd_out": [
      {
        "name": "tool_register",
        "property": {
          "tool": {
            "type": "string"
          }
        },
        "required": [
          "tool"
        ]
      }
    ]
  }
}