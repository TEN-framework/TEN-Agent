{
  "type": "extension",
  "name": "openai_chatgpt_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.4"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md"
    ]
  },
  "api": {
    "property": {
      "api_key": {
        "type": "string"
      },
      "frequency_penalty": {
        "type": "float64"
      },
      "presence_penalty": {
        "type": "float64"
      },
      "temperature": {
        "type": "float64"
      },
      "top_p": {
        "type": "float64"
      },
      "model": {
        "type": "string"
      },
      "max_tokens": {
        "type": "int64"
      },
      "base_url": {
        "type": "string"
      },
      "prompt": {
        "type": "string"
      },
      "greeting": {
        "type": "string"
      },
      "proxy_url": {
        "type": "string"
      },
      "max_memory_length": {
        "type": "int64"
      },
      "vendor": {
        "type": "string"
      },
      "azure_endpoint": {
        "type": "string"
      },
      "azure_api_version": {
        "type": "string"
      }
    },
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          }
        }
      }
    ],
    "cmd_in": [
      {
        "name": "flush"
      },
      {
        "name": "on_user_joined",
        "property": {}
      },
      {
        "name": "on_user_left",
        "property": {}
      },
      {
        "name": "tool_register",
        "property": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "parameters": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "description",
          "parameters"
        ],
        "result": {
          "property": {
            "response": {
              "type": "string"
            }
          }
        }
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      },
      {
        "name": "tool_call",
        "property": {
          "name": {
            "type": "string"
          },
          "args": {
            "type": "string"
          }
        },
        "required": [
          "name"
        ]
      }
    ],
    "video_frame_in": [
      {
        "name": "video_frame"
      }
    ]
  }
}