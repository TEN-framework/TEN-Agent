{
  "type": "extension",
  "name": "aisuite_llm_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.6"
    }
  ],
  "api": {
    "property": {
      "api_key": {
        "type": "string"
      },
      "model": {
        "type": "string"
      },
      "max_tokens": {
        "type": "int64"
      },
      "prompt": {
        "type": "string"
      },
      "greeting": {
        "type": "string"
      },
      "max_memory_length": {
        "type": "int64"
      },
      "provider_config": {
        "type": "string"
      }
    },
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          },
          "is_final": {
            "type": "bool"
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          },
          "end_of_segment": {
            "type": "bool"
          }
        }
      }
    ],
    "cmd_in": [
      {
        "name": "flush"
      },
      {
        "name": "call_chat",
        "property": {
          "messages": {
            "type": "string"
          },
          "stream": {
            "type": "bool"
          }
        },
        "required": [
          "messages"
        ],
        "result": {
          "property": {
            "text": {
              "type": "string"
            }
          },
          "required": [
            "text"
          ]
        }
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      }
    ]
  }
}