{
  "type": "extension",
  "name": "azure_vision_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.2"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md"
    ]
  },
  "api": {
    "property": {
      "key": {
        "type": "string"
      },
      "endpoint": {
        "type": "string"
      }
    },
    "cmd_in": [
      {
        "name": "analyze_image",
        "property": {
          "image_data": {
            "type": "buf"
          }
        },
        "required": [
          "image_data"
        ],
        "result": {
          "property": {
            "response": {
              "type": "string"
            }
          }
        }
      }
    ]
  }
}