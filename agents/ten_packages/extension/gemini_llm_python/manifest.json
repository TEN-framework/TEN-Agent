{
    "type": "extension",
    "name": "gemini_llm_python",
    "version": "0.4.0",
    "dependencies": [
        {
            "type": "system",
            "name": "ten_runtime_python",
            "version": "0.4"
        }
    ],
    "api": {
        "property": {
            "api_key": {
                "type": "string"
            },
            "greeting": {
                "type": "string"
            },
            "max_memory_length": {
                "type": "int64"
            },
            "max_output_tokens": {
                "type": "int64"
            },
            "model": {
                "type": "string"
            },
            "prompt": {
                "type": "string"
            },
            "temperature": {
                "type": "float64"
            },
            "top_k": {
                "type": "int64"
            },
            "top_p": {
                "type": "float64"
            }
        },
        "data_in": [
            {
                "name": "text_data",
                "property": {
                    "text": {
                        "type": "string"
                    }
                }
            }
        ],
        "data_out": [
            {
                "name": "text_data",
                "property": {
                    "text": {
                        "type": "string"
                    }
                }
            }
        ],
        "cmd_in": [
            {
                "name": "flush"
            }
        ],
        "cmd_out": [
            {
                "name": "flush"
            }
        ]
    }
}